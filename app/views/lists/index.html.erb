<div class="container">
  <h1>Your Lists</h1>
  <ul class = "p-0 my-2">
    <% @lists.each do |list| %>
      <li class= "list-group-item text-center my-2"><%= link_to list.name, list_path(list) %></li>
    <% end %>
  <%= link_to "Add a list!", new_list_path, class:"btn btn-secondary" %>
  </ul>
</div>
